  abstract = {The phytoplankton community in the oligotrophic open ocean is numerically dominated by the cyanobacterium Prochlorococcus, accounting for approximately half of all photosynthesis. In the illuminated euphotic zone where Prochlorococcus grows, reactive oxygen species are continuously generated via photochemical reactions with dissolved organic matter. However, Prochlorococcus genomes lack catalase and additional protective mechanisms common in other aerobes, and this genus is highly susceptible to oxidative damage from hydrogen peroxide (HOOH). In this study we showed that the extant microbial community plays a vital, previously unrecognized role in cross-protecting Prochlorococcus from oxidative damage in the surface mixed layer of the oligotrophic ocean. Microbes are the primary HOOH sink in marine systems, and in the absence of the microbial community, surface waters in the Atlantic and Pacific Ocean accumulated HOOH to concentrations that were lethal for Prochlorococcus cultures. In laboratory experiments with the marine heterotroph Alteromonas sp., serving as a proxy for the natural community of HOOH-degrading microbes, bacterial depletion of HOOH from the extracellular milieu prevented oxidative damage to the cell envelope and photosystems of co-cultured Prochlorococcus, and facilitated the growth of Prochlorococcus at ecologically-relevant cell concentrations. Curiously, the more recently evolved lineages of Prochlorococcus that exploit the surface mixed layer niche were also the most sensitive to HOOH. The genomic streamlining of these evolved lineages during adaptation to the high-light exposed upper euphotic zone thus appears to be coincident with an acquired dependency on the extant HOOH-consuming community. These results underscore the importance of (indirect) biotic interactions in establishing niche boundaries, and highlight the impacts that community-level responses to stress may have in the ecological and evolutionary outcomes for co-existing species.},
  langid = {english},
  keywords = {Catalases,Community ecology,Ecological niches,Marine ecology,Oceans,Photosynthesis,Sea water,Synechococcus},
  note = {\begin{itemize}

\item Prochlorococcus are susceptible to oxidative damage from hydrogen peroxide
\item Extant microbial community in ocean surface water plays important role of protecting them from lethal concentrations of hydrogen peroxide for prochlorococcus
\item Bacterial depletion of HOOH from environment prevented oxidative damage to prochlorococcus and facilitated the growth at ecologically relevant cell concentrations
\item High-light ecotypes are more sensitive to HOOH
\item HOOH stress results in detrimental loss of cell integrity and photosynthetic capacity
\item eMIT9313 has retained the relatively high resistance to HOOH

\end{itemize}},
  file = {C\:\\Users\\msavoie\\Zotero\\storage\\98JTMTKC\\Morris et al. - 2011 - Dependence of the Cyanobacterium Prochlorococcus o.pdf;C\:\\Users\\msavoie\\Zotero\\storage\\3Z79J3IN\\article.html}
}

@article{morrisFacilitationRobustGrowth2008,
  title = {Facilitation of {{Robust Growth}} of {{Prochlorococcus Colonies}} and {{Dilute Liquid Cultures}} by ``{{Helper}}'' {{Heterotrophic Bacteria}}},
  author = {Morris, J. Jeffrey and Kirkegaard, Robin and Szul, Martin J. and Johnson, Zackary I. and Zinser, Erik R.},
