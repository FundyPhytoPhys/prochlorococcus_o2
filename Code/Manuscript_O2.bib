@article{moorePhotophysiologyMarineCyanobacterium1999,
  title = {Photophysiology of the Marine Cyanobacterium {{Prochlorococcus}}: {{Ecotypic}} Differences among Cultured Isolates},
  shorttitle = {Photophysiology of the Marine Cyanobacterium {{Prochlorococcus}}},
  author = {Moore, Lisa R. and Chisholm, Sallie W.},
