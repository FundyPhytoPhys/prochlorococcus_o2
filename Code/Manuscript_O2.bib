  abstract = {Global ocean oxygen loss is projected to persist in the future, but Earth system models (ESMs) have not yet provided a consistent picture of how it will influence the largest oxygen minimum zone (OMZ) in the tropical Pacific. We examine the change in the Pacific OMZ volume in an ensemble of ESMs from the CMIP6 archive, considering a broad range of oxygen (O2) thresholds relevant to biogeochemical cycles and ecosystems (5{\textendash}160 {\textmu}mol/kg). Despite OMZ biases in the historical period of the simulations, the ESM ensemble projections consistently fall into three regimes across ESMs: an expansion of low oxygenated waters (+0.8 [0.6, 1.0] {\texttimes} 1016 m3/century for O2 {$\leq$} 120 {\textmu}mol/kg, ESM median and interquartile range); a slight contraction of the OMZ core although more uncertain across ESMs (-0.1 [-0.5, 0.0] {\texttimes} 1016 m3/century for O2 {$\leq$} 20 {\textmu}mol/kg); and at the transition from contraction to expansion regimes, a spatial redistribution but near-zero change in the volume of hypoxic waters (0.0 [-0.3, +0.1] {\texttimes} 1016 m3/century for O2 {$\leq$} 60 {\textmu}mol/kg). Changes in circulation and biology dictate the shift from expansion to contraction. Specifically, reduced subtropical ventilation controls the expansion of low oxygenated waters, while a combination of circulation and biological changes explains the contraction of the core (likely changes in mixing, reduced intermediate ventilation and oxygen demand). Increased model complexity (e.g., ecosystem dynamics and equatorial circulation) likely stabilize the OMZ response, suggesting that future changes might lie in the lower bound of current projections. The expansion of low oxygenated waters which delimit the optimum habitat of numerous marine species would severely impact ecosystems and ecosystem services.},
  copyright = {{\textcopyright} 2022. The Authors.},
  langid = {english},
  keywords = {climate change,CMIP6,ocean deoxygenation,OMZ,oxygen minimum zone,Pacific Ocean},
  note = {e2021AV000470 2021AV000470},
  file = {C\:\\Users\\msavoie\\Zotero\\storage\\46TKSPBM\\Busecke et al. - 2022 - Diverging Fates of the Pacific Ocean Oxygen Minimu.pdf;C\:\\Users\\msavoie\\Zotero\\storage\\ZWADB357\\2021AV000470.html}
}

@article{changBRENDAELIXIRCore2021,
  title = {{{BRENDA}}, the {{ELIXIR}} Core Data Resource in 2021: New Developments and Updates},
  shorttitle = {{{BRENDA}}, the {{ELIXIR}} Core Data Resource in 2021},
  author = {Chang, Antje and Jeske, Lisa and Ulbrich, Sandra and Hofmann, Julia and Koblitz, Julia and Schomburg, Ida and {Neumann-Schaal}, Meina and Jahn, Dieter and Schomburg, Dietmar},
  year = {2021},
  month = jan,
  journal = {Nucleic Acids Research},
  volume = {49},
  number = {D1},
  pages = {D498-D508},
  issn = {0305-1048},
  doi = {10.1093/nar/gkaa1025},
  urldate = {2024-02-23},
  abstract = {The BRENDA enzyme database (https://www.brenda-enzymes.org), established in 1987, has evolved into the main collection of functional enzyme and metabolism data. In 2018, BRENDA was selected as an ELIXIR Core Data Resource. BRENDA provides reliable data, continuous curation and updates of classified enzymes, and the integration of newly discovered enzymes. The main part contains \&gt;5 million data for {$\sim$}90 000 enzymes from {$\sim$}13 000 organisms, manually extracted from {$\sim$}157 000 primary literature references, combined with information of text and data mining, data integration, and prediction algorithms. Supplements comprise disease-related data, protein sequences, 3D structures, genome annotations, ligand information, taxonomic, bibliographic, and kinetic data. BRENDA offers an easy access to enzyme information from quick to advanced searches, text- and structured-based queries for enzyme-ligand interactions, word maps, and visualization of enzyme data. The BRENDA Pathway Maps are completely revised and updated for an enhanced interactive and intuitive usability. The new design of the Enzyme Summary Page provides an improved access to each individual enzyme. A new protein structure 3D viewer was integrated. The prediction of the intracellular localization of eukaryotic enzymes has been implemented. The new EnzymeDetector combines BRENDA enzyme annotations with protein and genome databases for the detection of eukaryotic and prokaryotic enzymes.},
  file = {C\:\\Users\\msavoie\\Zotero\\storage\\KQY62RHR\\Chang et al. - 2021 - BRENDA, the ELIXIR core data resource in 2021 new.pdf;C\:\\Users\\msavoie\\Zotero\\storage\\5WA68AKY\\5992283.html}
}

@article{chisholmNovelFreelivingProchlorophyte1988,
  title = {A Novel Free-Living Prochlorophyte Abundant in the Oceanic Euphotic Zone},
  author = {Chisholm, Sallie W. and Olson, Robert J. and Zettler, Erik R. and Goericke, Ralf and Waterbury, John B. and Welschmeyer, Nicholas A.},
  year = {1988},
  month = jul,
  journal = {Nature},
  volume = {334},
  number = {6180},
  pages = {340--343},
  publisher = {{Nature Publishing Group}},
  issn = {1476-4687},
  doi = {10.1038/334340a0},
  urldate = {2023-03-11},
  abstract = {The recent discovery of photosynthetic picoplankton has changed our understanding of marine food webs1. Both prokaryotic2,3 and eukaryotic4,5 species occur in most of the world's oceans and account for a significant proportion of global productivity6. Using shipboard flow cytometry, we have identified a new group of picoplankters which are extremely abundant, and barely visible using traditional microscopic techniques. These cells are smaller than the coccoid cyanobacteria and reach concentrations greater than 105 cells ml{\textendash}1 in the deep euphotic zone. They fluoresce red and contain a divinyl chlorophyll a-like pigment, as well as chlorophyll b, {$\alpha$}-carotene, and zeaxanthin. This unusual combination of pigments, and a distinctive prokaryotic ultrastructure, suggests that these picoplankters are free-living relatives of Prochloron7. They differ from previously reported prochlorophytes{\textemdash}the putative ancestors of the chloroplasts of higher plants{\textemdash}in that they contain {$\alpha$}-carotene rather than {$\beta$}-carotene and contain a divinyl chlorophyll a-like pigment as the dominant chlorophyll.},
  copyright = {1988 Nature Publishing Group},
  langid = {english},
  keywords = {Humanities and Social Sciences,multidisciplinary,Science}
}

@article{chisholmProchlorococcusMarinusNov1992,
  title = {Prochlorococcus Marinus Nov. Gen. Nov. Sp.: An Oxyphototrophic Marine Prokaryote Containing Divinyl Chlorophyll a and b},
  shorttitle = {Prochlorococcus Marinus Nov. Gen. Nov. Sp.},
  author = {Chisholm, Sallie W. and Frankel, Sheila L. and Goericke, Ralf and Olson, Robert J. and Palenik, Brian and Waterbury, John B. and {West-Johnsrud}, Lisa and Zettler, Erik R.},
