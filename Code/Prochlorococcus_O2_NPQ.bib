@article{steglichPhotophysicalPropertiesProchlorococcus2003,
  title = {Photophysical Properties of {{{\emph{Prochlorococcus}}}}{\emph{ Marinus}} {{SS120}} Divinyl Chlorophylls and Phycoerythrin in Vitro and in Vivo},
