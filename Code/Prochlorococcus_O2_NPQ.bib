@article{billerMarineMicrobialMetagenomes2018,
  title = {Marine Microbial Metagenomes Sampled across Space and Time},
  author = {Biller, Steven J. and Berube, Paul M. and Dooley, Keven and Williams, Madeline and Satinsky, Brandon M. and Hackl, Thomas and Hogle, Shane L. and Coe, Allison and Bergauer, Kristin and Bouman, Heather A. and Browning, Thomas J. and De Corte, Daniele and Hassler, Christel and Hulston, Debbie and Jacquot, Jeremy E. and Maas, Elizabeth W. and Reinthaler, Thomas and Sintes, Eva and Yokokawa, Taichi and Chisholm, Sallie W.},
