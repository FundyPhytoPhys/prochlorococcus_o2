  publisher = {{Nature Publishing Group}},
  issn = {2052-4463},
  doi = {10.1038/sdata.2018.154},
  urldate = {2023-08-08},
  abstract = {Prochlorococcus and Synechococcus are the dominant primary producers in marine ecosystems and perform a significant fraction of ocean carbon fixation. These cyanobacteria interact with a diverse microbial community that coexists with them. Comparative genomics of cultivated isolates has helped address questions regarding patterns of evolution and diversity among microbes, but the fraction that can be cultivated is miniscule compared to the diversity in the wild. To further probe the diversity of these groups and extend the utility of reference sequence databases, we report a data set of single cell genomes for 489 Prochlorococcus, 50 Synechococcus, 9 extracellular virus particles, and 190 additional microorganisms from a diverse range of bacterial, archaeal, and viral groups. Many of these uncultivated single cell genomes are derived from samples obtained on GEOTRACES cruises and at well-studied oceanographic stations, each with extensive suites of physical, chemical, and biological measurements. The genomic data reported here greatly increases the number of available Prochlorococcus genomes and will facilitate studies on evolutionary biology, microbial ecology, and biological oceanography.},
  copyright = {2018 The Author(s)},
  langid = {english},
  keywords = {Biogeography,Coevolution,Genome,Marine biology,Microbial ecology},
  file = {C:\Users\msavoie\Zotero\storage\6MA6PBAG\Berube et al. - 2018 - Single cell genomes of Prochlorococcus, Synechococ.pdf}
}

@article{bibbyLowlightadaptedProchlorococcusSpecies2003,
  title = {Low-Light-Adapted {{Prochlorococcus}} Species Possess Specific Antennae for Each Photosystem},
  author = {Bibby, T. S. and Mary, I. and Nield, J. and Partensky, F. and Barber, J.},
  year = {2003},
  month = aug,
  journal = {Nature},
  volume = {424},
  number = {6952},
  pages = {1051--1054},
  issn = {1476-4687},
  doi = {10.1038/nature01933},
  abstract = {Prochlorococcus, the most abundant genus of photosynthetic organisms, owes its remarkably large depth distribution in the oceans to the occurrence of distinct genotypes adapted to either low- or high-light niches. The pcb genes, encoding the major chlorophyll-binding, light-harvesting antenna proteins in this genus, are present in multiple copies in low-light strains but as a single copy in high-light strains. The basis of this differentiation, however, has remained obscure. Here we show that the moderate low-light-adapted strain Prochlorococcus sp. MIT 9313 has one iron-stress-induced pcb gene encoding an antenna protein serving photosystem I (PSI)--comparable to isiA genes from cyanobacteria--and a constitutively expressed pcb gene encoding a photosystem II (PSII) antenna protein. By comparison, the very low-light-adapted strain SS120 has seven pcb genes encoding constitutive PSI and PSII antennae, plus one PSI iron-regulated pcb gene, whereas the high-light-adapted strain MED4 has only a constitutive PSII antenna. Thus, it seems that the adaptation of Prochlorococcus to low light environments has triggered a multiplication and specialization of Pcb proteins comparable to that found for Cab proteins in plants and green algae.},
  langid = {english},
  pmid = {12944966},
  keywords = {Adaptation Physiological,Bacterial Proteins,Carrier Proteins,Cyanobacteria,Environment,Gene Dosage,Gene Expression Regulation Bacterial,Genes Bacterial,Genotype,Light,Light-Harvesting Protein Complexes,Photosynthetic Reaction Center Complex Proteins},
  note = {Show that the moderate low-light-adapted strain~\emph{Prochlorococcus}~sp. MIT 9313 has one iron-stress-induced~\emph{pcb}~gene encoding an antenna protein serving photosystem I (PSI)---comparable to~\emph{isiA}~genes from cyanobacteria---and a constitutively expressed~\emph{pcb}~gene encoding a photosystem II (PSII) antenna protein. By comparison, the very low-light-adapted strain SS120 has seven~\emph{pcb}~genes encoding constitutive PSI and PSII antennae, plus one PSI iron-regulated~\emph{pcb}~gene, whereas the high-light-adapted strain MED4 has only a constitutive PSII antenna.}
}

