@article{buseckeDivergingFatesPacific2022,
  title = {Diverging {{Fates}} of the {{Pacific Ocean Oxygen Minimum Zone}} and {{Its Core}} in a {{Warming World}}},
  author = {Busecke, Julius J. M. and Resplandy, Laure and Ditkovsky, Sam J. and John, Jasmin G.},
  year = {2022},
  journal = {AGU Advances},
  volume = {3},
  number = {6},
  pages = {e2021AV000470},
  issn = {2576-604X},
  doi = {10.1029/2021AV000470},
  urldate = {2023-08-27},
  abstract = {Global ocean oxygen loss is projected to persist in the future, but Earth system models (ESMs) have not yet provided a consistent picture of how it will influence the largest oxygen minimum zone (OMZ) in the tropical Pacific. We examine the change in the Pacific OMZ volume in an ensemble of ESMs from the CMIP6 archive, considering a broad range of oxygen (O2) thresholds relevant to biogeochemical cycles and ecosystems (5--160 {\textmu}mol/kg). Despite OMZ biases in the historical period of the simulations, the ESM ensemble projections consistently fall into three regimes across ESMs: an expansion of low oxygenated waters (+0.8 [0.6, 1.0] {\texttimes} 1016 m3/century for O2 {$\leq$} 120 {\textmu}mol/kg, ESM median and interquartile range); a slight contraction of the OMZ core although more uncertain across ESMs (-0.1 [-0.5, 0.0] {\texttimes} 1016 m3/century for O2 {$\leq$} 20 {\textmu}mol/kg); and at the transition from contraction to expansion regimes, a spatial redistribution but near-zero change in the volume of hypoxic waters (0.0 [-0.3, +0.1] {\texttimes} 1016 m3/century for O2 {$\leq$} 60 {\textmu}mol/kg). Changes in circulation and biology dictate the shift from expansion to contraction. Specifically, reduced subtropical ventilation controls the expansion of low oxygenated waters, while a combination of circulation and biological changes explains the contraction of the core (likely changes in mixing, reduced intermediate ventilation and oxygen demand). Increased model complexity (e.g., ecosystem dynamics and equatorial circulation) likely stabilize the OMZ response, suggesting that future changes might lie in the lower bound of current projections. The expansion of low oxygenated waters which delimit the optimum habitat of numerous marine species would severely impact ecosystems and ecosystem services.},
  copyright = {{\copyright} 2022. The Authors.},
  langid = {english},
  keywords = {climate change,CMIP6,ocean deoxygenation,OMZ,oxygen minimum zone,Pacific Ocean},
  note = {e2021AV000470 2021AV000470},
  file = {C\:\\Users\\msavoie\\Zotero\\storage\\46TKSPBM\\Busecke et al. - 2022 - Diverging Fates of the Pacific Ocean Oxygen Minimu.pdf;C\:\\Users\\msavoie\\Zotero\\storage\\ZWADB357\\2021AV000470.html}
}

