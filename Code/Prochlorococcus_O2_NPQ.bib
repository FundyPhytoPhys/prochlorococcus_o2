@techreport{huotDoesChlorophyllLt2007,
  type = {Preprint},
  title = {Does Chlorophyll \&lt;I\&gt;A\&lt;/I\&gt; Provide the Best Index of Phytoplankton Biomass for Primary Productivity Studies?},
  author = {Huot, Y. and Babin, M. and Bruyant, F. and Grob, C. and Twardowski, M. S. and Claustre, H.},
  year = {2007},
  month = mar,
  doi = {10.5194/bgd-4-707-2007},
  urldate = {2023-10-13},
  abstract = {Probably because it is a readily available ocean color product, almost all models of primary productivity use chlorophyll as their index of phytoplankton biomass. As other variables become more readily available, both from remote sensing and in situ au5 tonomous platforms, we should ask if other indices of biomass might be preferable. Herein, we compare the accuracy of different proxies of phytoplankton biomass for estimating the maximum photosynthetic rate (Pmax) and the initial slope of the production versus irradiance (P vs. E) curve ({$\alpha$}). The proxies compared are: the total chlorophyll a concentration (Tchla, the sum of chlorophyll a and divinyl chlorophyll), the phytoplank10 ton absorption coefficient, the phytoplankton photosynthetic absorption coefficient, the active fluorescence in situ, the particulate scattering coefficient at 650 nm (bp (650)), and the particulate backscattering coefficient at 650 nm (bbp(650)). All of the data (about 170 P vs. E curves) were collected in the South Pacific Ocean. We find that when only the phytoplanktonic biomass proxies are available, bp (650) and Tchla are 15 respectively the best estimators of Pmax and alpha. When additional variables are available, such as the depth of sampling, the irradiance at depth, or the temperature, Tchla becomes the best estimator of both Pmax and {$\alpha$}. From a remote sensing perspective, error propagation analysis shows that, given the current algorithms errors for estimating bbp(650), Tchla remains the best estimator of Pmax.},
  langid = {english},
  file = {C:\Users\msavoie\Zotero\storage\9ISVYETY\bgd-4-707-2007-print.pdf}
}

