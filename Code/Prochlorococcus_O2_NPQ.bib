@article{olsonSpatialTemporalDistributions1990,
  title = {Spatial and Temporal Distributions of Prochlorophyte Picoplankton in the {{North Atlantic Ocean}}},
  author = {Olson, Robert J. and Chisholm, Sallie W. and Zettler, Erik R. and Altabet, Mark A. and Dusenberry, Jeffrey A.},
  year = {1990},
