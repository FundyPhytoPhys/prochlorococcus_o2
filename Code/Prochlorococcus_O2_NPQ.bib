@article{komarekPhylogenyTaxonomySynechococcuslike2020,
  title = {Phylogeny and Taxonomy of {{Synechococcus-like}} Cyanobacteria},
  author = {Kom{\'a}rek, Ji{\v r}{\'i} and Johansen, Jeffrey R. and {\v S}marda, Jan and Struneck{\'y}, Otakar},
  year = {2020},
