@article{hasanQuinonedependentProtonTransfer2013,
  title = {Quinone-Dependent Proton Transfer Pathways in the Photosynthetic Cytochrome {\emph{b}} {\textsubscript{6}} {\emph{f}} Complex},
  author = {Hasan, S. Saif and Yamashita, Eiki and Baniulis, Danas and Cramer, William A.},
