  author = {Moore, Lisa R. and Coe, Allison and Zinser, Erik R. and Saito, Mak A. and Sullivan, Matthew B. and Lindell, Debbie and {Frois-Moniz}, Katya and Waterbury, John and Chisholm, Sallie W.},
  year = {2007},
