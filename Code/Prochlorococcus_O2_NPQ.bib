@article{boumanOceanographicBasisGlobal2006,
  title = {Oceanographic {{Basis}} of the {{Global Surface Distribution}} of {{Prochlorococcus Ecotypes}}},
  author = {Bouman, Heather A. and Ulloa, Osvaldo and Scanlan, David J. and Zwirglmaier, Katrin and Li, William K. W. and Platt, Trevor and Stuart, Venetia and Barlow, Ray and Leth, Ole and Clementson, Lesley and Lutz, Vivian and Fukasawa, Masao and Watanabe, Shuichi and Sathyendranath, Shubha},
  year = {2006},
  month = may,
  journal = {Science},
  volume = {312},
  number = {5775},
  pages = {918--921},
  publisher = {{American Association for the Advancement of Science}},
  issn = {0036-8075, 1095-9203},
  doi = {10.1126/science.1122692},
  urldate = {2020-05-06},
  abstract = {By using data collected during a continuous circumnavigation of the Southern Hemisphere, we observed clear patterns in the population-genetic structure of Prochlorococcus, the most abundant photosynthetic organism on Earth, between and within the three Southern Subtropical Gyres. The same mechanisms that were previously invoked to account for the vertical distribution of ecotypes at local scales accounted for the global (horizontal) patterns we observed. Basin-scale and seasonal variations in the structure and strength of vertical stratification provide a basis for understanding large-scale horizontal distribution in genetic and physiological traits of Prochlorococcus, and perhaps of marine microbial communities in general. A global census of an abundant photosynthetic marine bacterium reveals that its distribution is predicted by light, nutrients, and other oceanographic parameters. A global census of an abundant photosynthetic marine bacterium reveals that its distribution is predicted by light, nutrients, and other oceanographic parameters.},
  chapter = {Report},
  copyright = {American Association for the Advancement of Science},
  langid = {english},
  pmid = {16690867},
  note = {\begin{itemize}

\item Does genetic structure of prochlorococcus populations change in response to vertical mixing within and between the major ocean basins of the world
\item High light ecotype relies on reduced forms of nitrogen (ammonium and ammonia?)
\item Low light ecotypes can use both nitrite and ammonium
\item In the Pacific basin, the nutrient concentrations were high because of winter mixing so there are high concentrations of synechococcus and prochlorococcus cell abundance decreased 

\end{itemize}},
  file = {C\:\\Users\\msavoie\\Zotero\\storage\\9TD6V4MG\\Bouman et al. - 2006 - Oceanographic Basis of the Global Surface Distribu.pdf;C\:\\Users\\msavoie\\Zotero\\storage\\JJVB9HAA\\918.html}
}

@article{bresticLowPSIContent2015,
  title = {Low {{PSI}} Content Limits the Photoprotection of {{PSI}} and {{PSII}} in Early Growth Stages of Chlorophyll B-Deficient Wheat Mutant Lines},
  author = {Brestic, Marian and Zivcak, Marek and Kunderlikova, Kristyna and Sytar, Oksana and Shao, Hongbo and Kalaji, Hazem M. and Allakhverdiev, Suleyman I.},
  year = {2015},
  month = aug,
  journal = {Photosynthesis Research},
  volume = {125},
  number = {1},
  pages = {151--166},
  issn = {1573-5079},
  doi = {10.1007/s11120-015-0093-1},
  urldate = {2023-05-19},
  abstract = {In vivo analyses of electron and proton transport-related processes as well as photoprotective responses were carried out at different stages of growth in chlorophyll b (Chl b)-deficient mutant lines (ANK-32A and ANK-32B) and wild type (WT) of wheat (Triticum aestivum L.). In addition to a high Chl a--b ratio, ANK mutants had a lower content of photo-oxidizable photosystem I (PSI, Pm), and several parameters indicated a low PSI/PSII ratio. Moreover, simultaneous measurements of Chl fluorescence and P700 indicated a shift of balance between redox poise of the PSII acceptor side and the PSII donor side, with preferential reduction of the plastoquinone pool, resulting in an over reduced PSI acceptor side (high {$\Phi$}NA values). This was the probable reason for PSI inactivation observed in the ANK mutants, but not in WT. In later growth phases, we observed partial relief of ``chlorina symptoms,'' toward WT. Measurements of {$\Delta$}A520 decay confirmed that, in early growth stages, the ANK mutants with low PSI content had a limited capacity to build up the transthylakoid proton gradient ({$\Delta$}pH) needed to trigger non-photochemical quenching (NPQ) and to regulate the electron transport by cytochrome b6/f. Later, the increase in the PSI/PSII ratio enabled ANK mutants to reach full NPQ, but neither over reduction of the PSI acceptor side nor PSI photoinactivation due to imbalance between the activity of PSII and PSI was mitigated. Thus, our results support the crucial role of proper regulation of linear electron transport in the protection of PSI against photoinhibition. Moreover, the ANK mutants of wheat showing the dynamic developmental changes in the PSI/PSII ratio are presented here as very useful models for further studies.},
  langid = {english},
  keywords = {Chlorina mutants,Chlorophyll fluorescence,Non-photochemical quenching,PSI photoinhibition,Transthylakoid proton gradient,Wheat},
  file = {C:\Users\msavoie\Zotero\storage\W4SNPB5K\Brestic et al. - 2015 - Low PSI content limits the photoprotection of PSI .pdf}
}

